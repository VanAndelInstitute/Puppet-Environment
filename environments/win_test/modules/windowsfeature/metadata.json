{
  "name": "puppet-windowsfeature",
  "version": "1.1.0",
  "author": "puppetcommunity",
  "summary": "A module that will turn windows features on or off for Windows Server 2008 and above",
  "license": "Apache-2.0",
  "source": "https://github.com/puppet-community/puppet-windowsfeature",
  "project_page": "https://github.com/puppet-community/windowsfeature",
  "issues_url": "https://github.com/puppet-community/puppet-windowsfeature/issues",
  "tags": [
    "windows",
    "utility",
    "powershell"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "windows",
      "operatingsystemrelease": [
        "2008",
        "2008R2",
        "2012",
        "2012R2"
      ]
    }
  ],
  "dependencies": [
    {"name":"puppetlabs/stdlib","version_requirement":">=3.0.0 <5.0.0"},
    {"name":"puppetlabs/powershell","version_requirement":">= 1.0.1"}
  ]
}
