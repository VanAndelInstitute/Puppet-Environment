dsc_<%= dsc_type %> {'<%= dsc_type %>_test':
  <% dsc_props.each do |k, v| %>
    <% if v =~ /^\[.*\]$/ %>
      <%= k %> => <%= v %>,
    <% elsif v =~ /^(true|false)$/ %>
      <%= k %> => <%= v %>,
    <% elsif v =~ /^{.*}$/ %>
      <%= k %> => <%= v %>,
    <% else %>
      <%= k %> => '<%= v %>',
    <% end %>
  <% end %>
}
